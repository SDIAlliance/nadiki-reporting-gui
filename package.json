{
  "name": "nadiki-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "api-typegen": "npx openapicmd typegen https://raw.githubusercontent.com/SDIAlliance/nadiki-api/refs/heads/main/data-center-facility/facility-api.spec.yaml > ./types/registrar-api/facility-api.ts && npx openapicmd typegen https://raw.githubusercontent.com/SDIAlliance/nadiki-api/refs/heads/main/rack/rack-api.spec.yaml > ./types/registrar-api/rack-api.ts && npx openapicmd typegen https://raw.githubusercontent.com/SDIAlliance/nadiki-api/refs/heads/main/server/server-api.spec.yaml > ./types/registrar-api/server-api.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@opennextjs/cloudflare": "^1.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.513.0",
    "next": "^15.3.3",
    "openapi-client-axios": "^7.6.0",
    "openapicmd": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.0",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "wrangler": "^4.19.1"
  }
}
